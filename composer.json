{
  "name": "byteperfect/wp-background-processing",
  "description": "WP Background Processing can be used to fire off non-blocking asynchronous requests or as a background processing tool, allowing you to queue tasks.",
  "type": "library",
  "require": {
    "php": ">=7.4",
    "ext-json": "*"
  },
  "suggest": {
    "coenjacobs/mozart": "Easily wrap this library with your own prefix, to prevent collisions when multiple plugins use this library"
  },
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Delicious Brains",
      "email": "nom@deliciousbrains.com"
    },
    {
      "name": "Aleksandr Levashov",
      "email": "aleksandr@byteperfect.dev",
      "homepage": "https://byteperfect.dev/",
      "role": "Developer"
    }
  ],
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "require-dev": {
    "wp-coding-standards/wpcs": "^3.0",
    "phpstan/phpstan": "^1.10",
    "phpstan/extension-installer": "^1.3",
    "phpstan/phpstan-strict-rules": "^1.5",
    "php-stubs/wordpress-stubs": "^6.3",
    "php-stubs/woocommerce-stubs": "^8.2",
    "php-stubs/acf-pro-stubs": "^6.1",
    "szepeviktor/phpstan-wordpress": "^1.3"
  }
}
